{
  "name": "open2json",
  "version": "1.0.0",
  "description": "Collect and merge elements from wikidata and OpenStreetMap",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc && webpack",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "files": [
    "lib/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mmmatthew/open2json.git"
  },
  "keywords": [],
  "author": "Matthew Moy de Vitry",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/mmmatthew/open2json/issues"
  },
  "homepage": "https://github.com/mmmatthew/open2json#readme",
  "devDependencies": {
    "@types/jest": "^24.0.19",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.2.0",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.6.4",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9"
  },
  "dependencies": {
    "@types/geojson": "^7946.0.7",
    "axios": "^0.19.0",
    "query-overpass": "^1.5.4"
  }
}
